{
  "name": "spina-me",
  "version": "0.1.3",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "dev": "concurrently --kill-others \"nodemon server\" \"npm run ng-watch\" \"npm run watch-sass\"",
    "build": "npm run build-sass && ng build",
    "ng-watch": "ng build --watch --progress=false",
    "build-sass": "./node_modules/node-sass/bin/node-sass ./src/assets/scss/styles.scss -o ./src",
    "watch-sass": "./node_modules/node-sass/bin/node-sass --watch ./src/assets/scss/styles.scss -o ./src",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.3.1",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.3.1",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@types/jquery": "3.3.31",
    "angular2-materialize": "^15.1.3",
    "bcryptjs": "^2.4.3",
    "concurrently": "^3.5.0",
    "config": "^3.3.8",
    "core-js": "^2.4.1",
    "dotenv": "^4.0.0",
    "express-session": "^1.15.4",
    "express-validator": "^3.2.1",
    "jquery": "3.3.1",
    "materialize-css": "^0.100.1",
    "mongoose": "^4.11.4",
    "mongoose-unique-validator": "^1.0.5",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "15.0.4",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "2.5.45",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.0.1",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
